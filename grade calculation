#include<stdio.h>
#include<conio.h>
void main()
{
int marks;
printf("enter the marks: );
scanf("%d",&marks);
if(marks>=85 && marks<=100)
{
printf("grade A");
}
elseif(marks>=70 && marks<=84)
{
printf("grade B");
}
elseif(marks>=55 && marks<=69)
{
printf("grade C");
}
elseif(marks>=40 && marks<=54)
{
printf("grade D");
}
else // marks<40
{
printf("grade F");
}
getch();
}

